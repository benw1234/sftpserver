!echo 1 > one
!echo 2 > two
!ln -s two link
!ln -s three dangling
put -e one nonesuch
#.*file does not exist.*
put -f one link
#.*too many symbolic links.*
put -f one dangling
#.*too many symbolic links.*
put -ft one link
#.*too many symbolic links.*
put -ft one dangling
#.*too many symbolic links.*
put -fe one link
#.*too many symbolic links.*
put -fe one dangling
#.*too many symbolic links.*
